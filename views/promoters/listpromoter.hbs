<table id="example" class="table table-striped table-bordered" style="width:100%">
    <thead>
    <tr>
        <th>Nome</th>
        <th>Telefone</th>
        <th>Email</th>
        <th>Opcoes</th>
    </tr>
    </thead>
    <tbody>
    {{#if promoters}}
        {{#each promoters}}
            <tr>
                <td>{{name}}</td>
                <td>{{phoneNumber}}</td>
                <td>{{email}}</td>
                <td><a href="/newevent/{{_id}}">Criar Evento</a>, <a href="/backend/promoters/edit/{{_id}}">Editar</a></td>
            </tr>
        {{/each}}

    {{/if}}
    </tbody>
</table>